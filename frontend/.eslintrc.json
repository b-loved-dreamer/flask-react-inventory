{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "never"],
        "react/prop-types": ["off", {}],
        "react/jsx-props-no-spreading": ["off", {}],
        "jsx-a11y/label-has-associated-control": ["off", {}]

    }
}
